    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "@alloy-js/cli": ">=0.17.0",
    "@alloy-js/rollup-plugin": "^0.1.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
