      "types": "./dist/src/testing/main.d.ts",
      "default": "./dist/src/testing/main.js"
    }
  },
  "peerDependencies": {
    "@typespec/compiler": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
